                <div id="carouselCouchEvents" class="carousel slide pb-2 carouselTop" data-ride="carousel">
                    <ol class="carousel-indicators">
                        {{!-- according to HandleBars, if the value for the #if statement is false, undefined, null, "", 0, or [], Handlebars will not render the block. --}}
                        {{#each couchEvents}}
                            {{#if slideId}}<li data-target="#carouselCouchEvents" data-slide-to="{{slideId}}"></li>{{else}}<li data-target="#carouselCouchEvents" data-slide-to="{{slideId}}" class="active"></li>{{/if}}
                        {{/each}}
                        
                    </ol>
                    <div class="carousel-inner">

                        {{#each couchEvents}}
                            {{#if slideId}}<div class="carousel-item">{{else}}<div class="carousel-item active">{{/if}}
                                <img class="d-block w-100" src="assets/images/couchSlide{{slideId}}.png" alt="slide {{slideId}}">
                                <div class="carousel-caption d-none d-md-block text-right pr-3 pb-1">
                                    <h5>{{slideTitle}}</h5>
                                    <p>{{slideText}}</p>
                                </div>
                            </div>
                        {{/each}}

                    </div>

                    <a class="carousel-control-prev" href="#carouselCouchEvents" role="button" data-slide="prev">
