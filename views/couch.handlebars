    {{!-- NAVIGATION////////// --}}
    <nav class="navbar navbar-expand-lg navbar-dark bg-nav">
        <a class="navbar-brand pt-0 pb-0" href="/" style="color: #f7ae3f";><img src="/assets/images/TALogo2.png" width="80px">&nbsp;&nbsp;TOGETHER:AGAINST</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
            <div class="navbar-nav">
                <a class="nav-link active" href="/event/couch">Couch Events</a>
                <a class="nav-link" href="/event/table">Table Events</a>
                <a class="nav-link" href="/event/turf">Turf Events</a>            
