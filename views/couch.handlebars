    {{!-- MAIN////////// --}}
    <div id="main">
        <div class="row py-3">
            <div class="col-6 text-center my-auto">
                <h1 class="eventTitleBig">{{dbevent.title}}</h1>
            </div>
            <div class="col-6 text-center">
                <img src="/assets/images/CouchAnglefull.png" width="500px" >
            </div>
        </div>
        <div class="row eventOne">
            {{!-- TEAM ONE////////// --}}
            <div class="col-6 text-center pl-4 pr-0">
                <div class="row pb-1 px-3">
                    <div class="col-12 couchEventHeader">
                        {{dbevent.team_one_name}}
                    </div>
                    <div id="oppon1"></div>
                </div>
                <!-- STAT BOX-->
                <div class="row eventStats px-3 h-100">
                    <div class="col-12 py-2 event-turf">
                        <div class="tab-pane text-left py-3 px-2" id="eventStats1" width="200px">
                            <p class="statsText1 py-2 px-2">
                                {{dbevent.team_one_stat}}
                            </p>
                        </div>
                    </div>
                </div>
            </div>
            {{!-- TEAM TWO////////// --}}
            <div class="col-6 text-center pr-4 pl-0">
                <div class="row pb-1 px-3">
                    <div class="col-12 couchEventHeader">
                        {{dbevent.team_two_name}}
                    </div>
                    <div id="oppon1"></div>
                </div>
                <!-- STAT BOX-->
                <div class="row eventStats px-3 h-100">
                    <div class="col-12 py-2 event-turf">
                        <div class="tab-pane text-left py-3 px-2" id="eventStats1" width="200px">
                            <p class="statsText1 py-2 px-2">
                                {{dbevent.team_two_stat}}
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="pl-3 pr-3 pb-1 pt-1 mb-1">
            {{!-- VOTING////////// --}}
            <form class="text-center pb-2 mb-0">
                <div class="form-group ml-1 mr-1 pl-3 pr-3 mt-4">
                    <input class="btn btn-warning float-left" id="vote-one" data-id={{dbevent.id}} type="submit" value="Vote for {{dbevent.team_one_name}}!">
                    <input id="teamOneVote" type="text" readonly class="btn btn-light float-left" value="{{dbevent.team_one_votes}}">
                    <input class="btn btn-warning float-right" id="vote-two" data-id={{dbevent.id}} type="submit" value="Vote for {{dbevent.team_two_name}}!">
                    <input id="teamTwoVote" type="text" readonly class="btn btn-light float-right" value="{{dbevent.team_two_votes}}">
                </div>
            </form>
            <br>
        </div>
        {{!-- COMMENTS////////// --}}
        <div class="row pl-4 pr-4 pb-1 pt-1">
            <div class="col-12 text-center couchEventHeader">
                BACK UP YOUR PROJECTED WINNER
            </div>
        </div>       
        <div class="row pl-4 pr-4 pb-1 pt-1">
            <div class="col-12">
                <div id="disqus_thread"></div>
            </div>
        </div>                             
        <br>
        <div class="row pl-4 pr-4 pb-1 pt-1">
            <div class="col-12 couchEventHeader">
                <br>
            </div>
        </div>
    </div>
    {{!-- FOOTER////////// --}}
    <footer class="footer pt-3 pb-3">
        <div class="container text-center">
            <span>TOGETHER:AGAINST 2020</span>
        </div>
    </footer>
